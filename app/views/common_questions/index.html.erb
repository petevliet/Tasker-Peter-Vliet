<a id = "top" > </a>
  <div class= "page-header">
    <h1>Frequently Asked Questions</h1>
  </div>

<ul>
  <% @questions.each do |inquire| %>
  <div>
    <li>
      <%= link_to inquire.question, "##{inquire.slug}" %>
    </li>
  </div>
  <% end %>
</ul>

<div class ="row">
  <div class="col-md-6">
  <% @questions.each do |faq| %>
    <div><h3><%= faq.question %></h3></div>
    <a name= "<%= faq.slug %>"></a>
    <p></p>
    <div><%= faq.answer %></div>
    <p></p>
    <%= link_to "Top", "#top" %>
    <% end %>
  </div>
</div>
